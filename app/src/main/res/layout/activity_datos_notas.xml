        <TextView
            android:id="@+id/tvMatT2"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="18dp"
            android:text="TextView"
            android:textAlignment="center"
            android:textColor="#404040"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/tvMatT1"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />

        <TextView
            android:id="@+id/tvBioT3"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:text="TextView"
            android:textAlignment="center"
            android:textColor="#404040"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/tvBioT2"
            app:layout_constraintTop_toBottomOf="@+id/tvLengT3" />

        <TextView
            android:id="@+id/tvLengT3"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:text="TextView"
            android:textAlignment="center"
            android:textColor="#404040"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/tvLengT2"
            app:layout_constraintTop_toBottomOf="@+id/tvMatT3" />

        <TextView
            android:id="@+id/tvBioT2"
            android:layout_width="60dp"
            android:layout_height="25dp"

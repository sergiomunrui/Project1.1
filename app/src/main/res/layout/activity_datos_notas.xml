        android:layout_height="346dp"
        android:background="#E5FCFB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.45">

        <TextView
            android:id="@+id/tvEfT2"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:text="TextView"
            android:textAlignment="center"
            android:textColor="#404040"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/tvEfT1"
            app:layout_constraintTop_toBottomOf="@+id/tvIngT2" />

        <TextView
            android:id="@+id/tvMatT3"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="18dp"
            android:text="TextView"
            android:textAlignment="center"
            android:textColor="#404040"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/tvMatT2"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />

        <TextView
            android:id="@+id/tvEfT1"
            android:layout_width="60dp"

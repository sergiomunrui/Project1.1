<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1DB7AE"
    android:textAlignment="center"
    tools:context=".DatosNotasActivity">

    <ImageView
        android:id="@+id/bgNombre"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toEndOf="@+id/bgId"
        app:layout_constraintTop_toBottomOf="@+id/tvPantallaNotas"
        app:srcCompat="@drawable/bottom_background_light_green" />

    <ImageView
        android:id="@+id/bgId"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPantallaNotas"
        app:srcCompat="@drawable/bottom_background_light_green" />

    <TextView
        android:id="@+id/tvIdAlumno"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:textAlignment="center"
        android:textColor="#404040"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bgId"
        app:layout_constraintEnd_toEndOf="@+id/bgId"
        app:layout_constraintStart_toStartOf="@+id/bgId"
        app:layout_constraintTop_toTopOf="@+id/bgId" />

    <TextView
        android:id="@+id/tvPantallaNotas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="@string/tvPantallaNota"
        android:textAlignment="center"
        android:textColor="#FEDD06"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvNombre"
        android:layout_width="250dp"
        android:layout_height="25dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#404040"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bgNombre"
        app:layout_constraintEnd_toEndOf="@+id/bgNombre"
        app:layout_constraintStart_toStartOf="@+id/bgNombre"
        app:layout_constraintTop_toTopOf="@+id/bgNombre" />

    <android.support.constraint.ConstraintLayout
